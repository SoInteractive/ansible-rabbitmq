[[inputs.rabbitmq]]
  url = "http://localhost:15672"
{% for user in rabbitmq_users %}
{%   if user.tag == "administrator" %}
  username = "{{ user.user }}"
  password = "{{ user.password }}"
{%   endif %}
{% endfor %}
